{
    "manifest_version": 2,
    "name": "Tự fill đáp án",
    "version": "1.0",
    "description": "Base on NHV's code",
    "permissions": [
      "activeTab",
      "https://raw.githubusercontent.com/"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": ["https://*/*", "http://*/*"],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "48": "icon.png"
    },
    "permissions": [
        "activeTab",
        "storage"
    ]
    
  }
  